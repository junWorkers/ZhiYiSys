@charset "utf-8";
/* CSS Document */

*{
	padding:0;
	margin:0 auto;
}
a{
	color:#999;
	text-decoration:none;
	cursor:pointer;
	outline: 0 none;
	}
#fa-header{
	width:100%;
	height:61px;
	background:#fff;
	position:fixed;  /*固定位置*/
	border-bottom:#CCC 1px solid;
	z-index:1000;
}
#fa-center{
	float:left;
	padding-left:170px;
}
.fa-logo{
	float:left;	
}
.fa-logo img{
	padding-top:12px;
	width:120px;
}


ul,li{
		margin:0;
		padding:0;
}
.fa-centers {
		list-style:none;
		height:60px;
		line-height:30px;
		float:left;	
		padding-top:20px;
}
.fa-centers li{
	float:left;
}
.fa-centers li a{
		display:inline-block;
		width:80px;
		height:30px;
		text-decoration:none;
		font-size:16px;
		color:#999;
}
#fa-center ul li a:hover {
    color: #F00;
    text-decoration-color: -moz-use-text-color;
    text-decoration-line: none;
    text-decoration-style: solid;
}

.fa-centers .first1{
	padding-left:45px;
}
.fa-centers .last1{
	padding-left:30px;
}
#fa-center .img1{
	padding-top:10px;
	padding-left:20px;
	float:left;
}
.fa-right{
	list-style:none;
	height:60px;
	line-height:30px;
	float:left;	
	padding-top:20px;
}
.fa-right li{
	float:left;
}
.fa-right li a{
	display:inline-block;
	height:30px;
	text-decoration:none;
	font-size:14px;
	color:#999;
	padding-left:18px;
}
/*首部结束*/
.bg-fetchpassword{
	padding-top:61px;
	background-image:url(../images/bg_services.jpg);
	width:100%;
	height:180px;
	background-origin: padding-box;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: auto auto;
}
.bg-nav{
	padding-left:43%;
	padding-top:95px;
}
.bg-nav ul li{
	float:left;
	list-style:none;
	font-size:12px;
	color:#fff;
}
.bg-nav ul li a{
	color:#fff;
}
.bg-nav ul li a:hover{
	color:#F00;
}
/*------------------*/
.wrapper{
	margin-top: 30px;
}
.loginHeader{
	font-size: 18px;
	color:#444;
	padding-left:43%;
	border-bottom-color: #e0e0e0;
    border-bottom-style: solid;
    border-bottom-width: 1px;
	border-bottom:1px solid #ececec;
    text-align: left;
}
    
.loginHeader h1{
    color: #F00;
    display: inline-block;
    font-size: 16px;
    letter-spacing: 2px;
    margin-bottom: -1px;
    padding-bottom: 9px;
}
.tc p{
	padding-left:30%;
	font-size:12px;
	padding-top:10px;
	color:#666;
}

dt{
	float:left;
	padding-left:30%;
	font-size:14px;
	padding-top:30px;
	color:#666;
}
dd{
	padding-top:20px;
	float:left;
	padding-left:20px;
}
dl{
	clear:both;
}
.text{
	width:330px;
	height:40px;
}
.buttons{
	padding-left:10%;
	margin-bottom:20px;
}
.btn {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: #e8380d;
    background-image: none;
    background-origin: padding-box;
    background-position: 0 0;
    background-repeat: repeat;
    background-size: auto auto;
    border-bottom-color: -moz-use-text-color;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-style: none;
    border-bottom-width: medium;
    border-image-outset: 0 0 0 0;
    border-image-repeat: stretch stretch;
    border-image-slice: 100% 100% 100% 100%;
    border-image-source: none;
    border-image-width: 1 1 1 1;
    border-left-color: -moz-use-text-color;
    border-left-style: none;
    border-left-width: medium;
    border-right-color: -moz-use-text-color;
    border-right-style: none;
    border-right-width: medium;
    border-top-color: -moz-use-text-color;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-top-style: none;
    border-top-width: medium;
    color: #fff;
    cursor: pointer;
    float: left;
    font-size: 14px;
    height: 30px;
    margin-top: 0;
    text-align: center;
    width: 96px;
}
.verification {
    position: absolute;
    width: 171px;
    z-index: 9999;
}
.verification a {
    display: block;
    line-height: 18px;
    text-align: center;
}
.verification .left {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: rgba(0, 0, 0, 0);
    background-image: url("../images/51dfc75f0a1bb59b4d000d8c.png");
    background-origin: padding-box;
    background-position: -249px -202px;
    background-repeat: no-repeat;
    background-size: auto auto;
    float: left;
    height: 99px;
    width: 19px;
}
.verification .content {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: #fff;
    background-image: none;
    background-origin: padding-box;
    background-position: 0 0;
    background-repeat: repeat;
    background-size: auto auto;
    border-bottom-color: #e1e1e1;
    border-bottom-style: solid;
    border-bottom-width: 1px;
    border-top-color: #e1e1e1;
    border-top-style: solid;
    border-top-width: 1px;
    float: left;
    height: 97px;
    margin-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    text-align: center;
    width: 120px;
}
.verification .right {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: rgba(0, 0, 0, 0);
    background-image: url("../images/51dfc75f0a1bb59b4d000d8c.png");
    background-origin: padding-box;
    background-position: -265px -202px;
    background-repeat: no-repeat;
    background-size: auto auto;
    float: left;
    height: 99px;
    width: 12px;
}
/*尾部*/
#nb-footer{
	width:100%;
	height:50px;
	float:left;
	border-top:#CCC 1px solid;
}

#nb-footer{
	padding: 30px 0 50px 0; 
	margin: auto; z-index: 9; /*font-family: 'Shscn';*/
}
.nubia-copyright{
	margin: auto; 
	width: 1000px; 
	color: #ccc; 
	position: relative; 
	font-size: 12px;
}
.nubia-copyright .nubia-foot-nav {
	padding-top:8px; 
	-webkit-transform: scale(0.83333333) translateX(-99px);
	-moz-transform: scale(0.83333333) translateX(-99px);
	-ms-transform: scale(0.83333333) translateX(-99px);
	transform: scale(0.83333333) translateX(-99px);
}
.nubia-copyright .nubia-foot-nav a{
	padding: 0 15px; color: #999; 
	-webkit-transition: color .3s ease-in-out; 
	-moz-transition: color .3s ease-in-out; 
	-ms-transition: color .3s ease-in-out; 
	transition: color .3s ease-in-out;
}
.nubia-copyright .nubia-foot-nav a:first-child{
	padding-left: 0;
}
.nubia-copyright .nubia-foot-nav a:hover{
	color: #333;
}
.nubia-copyright .nb-footer-info{
	padding-top: 10px; 
	-webkit-transform: scale(0.83333333) translateX(-99px);
	-moz-transform: scale(0.83333333) translateX(-99px);
	-ms-transform: scale(0.83333333) translateX(-99px);
	transform: scale(0.83333333) translateX(-99px);
}
.nubia-copyright .nb-footer-info a{
	display: inline-block; 
	margin-right: 20px; 
	color: #ccc; 
	-webkit-transition: color .3s ease-in-out; 
	-moz-transition: color .3s ease-in-out; 
	-ms-transition: color .3s ease-in-out; 
	transition: color .3s ease-in-out;
}
.nubia-copyright .nb-footer-info a:hover{
	color: #333;
}
.nubia-copyright span.nb-footer-func{
	position: absolute; 
	top: 0; 
	right: 0;
}
.nubia-copyright span.nb-footer-func a{
	width: 30px; 
	height: 30px; 
	overflow: hidden; 
	display: inline-block; 
	margin-left: 10px; 
	line-height: 1em; 
	position: relative;
}
.nubia-copyright span.nb-footer-func a.nb-en-ico{
	background-position: -96px 0;
}
.nubia-copyright span.nb-footer-func a.nb-weibo-ico{
	background-position: -126px 0;
}
.nubia-copyright span.nb-footer-func a.nb-weixin-ico{
	background-position: -156px 0;
}
.nubia-copyright span.nb-footer-func a.nb-apk-ico{
	background-position: -246px 0;
}
.nubia-copyright span.nb-footer-func a.nb-cloud-ico{
	background-position: -186px 0;
}
.nubia-copyright span.nb-footer-func a.nb-service-ico{
	background-position: -216px 0;
}
.nubia-copyright span.nb-footer-func a.nb-weibo-ico:hover{
	background-position: -126px -30px;
}
.nubia-copyright span.nb-footer-func a.nb-weixin-ico:hover{
	background-position: -156px -30px;
}
.nubia-copyright span.nb-footer-func a.nb-apk-ico:hover{
	background-position: -246px -30px;
}
.nubia-copyright span.nb-footer-func a.nb-cloud-ico:hover{
	background-position: -186px -30px;
}
.nubia-copyright span.nb-footer-func a.nb-service-ico:hover{
	background-position: -216px -30px;
}
.nubia-copyright span.nb-footer-func strong{
	width: 220px; 
	height: 30px; 
	display: inline-block; 
	vertical-align: middle; 
	overflow: hidden; 
	margin-left: 10px;
}
span.nb-footer-func .nb-footer-wx-layer{
	width: 150px; 
	height: 150px; 
	background: #fff; 
	border:1px #ccc solid; 
	position: absolute; 
	right: 219px; 
	bottom: 45px; 
	border-radius: 5px; 
	display: none;
}
span.nb-footer-func .nb-footer-wx-layer img{
	width: 130px; 
	height: 130px; 
	position: absolute; 
	top: 11px; 
	left: 10px; 
	z-index: 1;
}
span.nb-footer-func .nb-footer-wx-layer:before{
	width: 19px; 
	height: 10px; 
	overflow: hidden; 
	position: absolute; 
	bottom: -10px; 
	left: 50%; 
	margin-left: -9px; 
	content: ''; 
	z-index: 2; 
	background-size: 19px 10px; 
	background-repeat: no-repeat;
}

.foot-link-tel {
    float: right;
    font-size: 12px;
    padding-right: 60px;
}
.foot-link-tel a {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: #e8340e;
    background-image: none;
    background-origin: padding-box;
    background-position: 0 0;
    background-repeat: repeat;
    background-size: auto auto;
    border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    color: #fff;
    display: block;
    height: 30px;
    line-height: 30px;
    margin-bottom: 30px;
    text-align: center;
    width: 140px;
}




