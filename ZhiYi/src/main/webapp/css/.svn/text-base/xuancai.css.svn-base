.s8{background:url(../images/zx300-s8-bg.jpg) center center no-repeat; background-size: 1920px 1000px;}
.s8 .section{height: 1000px;}
.s8 .title{width: 490px; padding-top: 350px;margin-right: 50px; margin-left:780px;}
.s8 .title h2{color: #333;font-size: 48px; font-weight: 400;}
.s8 .title h3{color: #333;font-size: 48px; font-weight: 200; }
