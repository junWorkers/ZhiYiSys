.s9{background: url(../images/zx300-s9-bg.jpg) center center no-repeat; background-size: 1920px 1150px;}
.s9 .section{height: 1150px;}
.s9 .title{width: 200px; padding-top: 120px; left: 420px; margin-left:560px;}
.s9 .title h2{color: #ccc;font-size: 48px;font-weight: 600;}
.s9 .title p.stitle{text-align: left; color: #ccc;}
.s9 .title ul{margin: 0 auto;margin-top: 90px;margin-left:200px;width: 300px;}
.s9 .title ul li{display: inline-block;width: 100%;height: 70px; float: left;}
.s9 .title ul li:nth-child(6){margin-top: 25px;}
