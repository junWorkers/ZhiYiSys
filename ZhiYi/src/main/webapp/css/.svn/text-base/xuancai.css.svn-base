@charset "utf-8";
/* CSS Document */

.high{background: #d5732a; overflow:hidden;}
.high .section{height: 300px; padding-top:190px;}
.high .title{width: 100%; left: 0; text-align: center; color: #fff;}
.high .title h2{color: #fff; font-size: 46px; font-weight: 200;}
.high .title h3{color: #fff; font-size: 40px; font-weight: 100; padding-top: 0;}
.high .title p.stitle{width: 420px; margin: 0 auto; color: #fff; text-align: center; padding-top: 15px; font-weight:100;}
.high .title a.readmore-btn{width: 8em; padding: 8px; border: 2px #fff solid; text-align: center; display: inline-block; font-size: 18px; color: #fff; margin-top: 40px; font-weight: 400;}
