.s9 .title ul li span{font-size: 16px;color: #666;line-height: 1.5em;}
.s9 .title ul li p{font-size: 16px;color: #999;}

.nb-purchase-popup{background-color: #fff; border-radius: 8px; width: 800px; height: 500px; overflow: hidden; position: fixed; left: 50%; margin-left: -400px; top: 150px; z-index: 99; box-shadow: 0 10px 50px rgba(0,0,0,0.3); display: none;}
.nb-purchase-popup img{  margin-right:4px;}
.nb-purchase-popup a.nb-p-close{width: 40px; height: 40px; position: absolute; top: 10px; right: 10px; -webkit-transition: opacity .3s ease; -moz-transition: opacity .3s ease; transition: opacity .3s ease;}
.nb-purchase-popup a.nb-p-close:hover{opacity: .8;}
.nb-purchase-popup .figure-area{width: 410px; height: 410px; position: absolute; top: 50%; left: 20px; margin-top: -205px; overflow: hidden;}
.nb-purchase-popup .figure-area img{width: 100%; height: 100%; position: absolute; top: 0; left: 0; z-index: 1; display: none;}
.nbp-goods-info{margin-left: 440px; margin-right: 40px; padding-top: 50px; color: #333;}
.nbp-goods-info h2.title{font-size: 24px; line-height: 1.4em;}
.nbp-goods-info p.subtitle{font-size: 18px; line-height: 1.2em; margin-top: 5px;}
.nbp-goods-info dl{display: block; margin-top: 30px; font-size: 16px;}
.nbp-goods-info dl dt{display: block; margin-bottom: 5px;}
.nbp-goods-info dl.nbp-goods-color dd a{display: inline-block; width: 24px; height: 24px; overflow: hidden; border-radius: 50%; margin-right: 5px; border: 2px #fff solid; position: relative; top: 0; left: 0;}
.nbp-goods-info dl.nbp-goods-color dd a em{display: inline-block; width: 24px; height: 24px; overflow: hidden; border-radius: 50%;}
.nbp-goods-info dl.nbp-goods-color dd a.border em{border: 1px #ccc solid; width: 22px; height: 22px;}
.nbp-goods-info dl.nbp-goods-color dd a.current{border-color: #e7380d;}
.nbp-goods-info dl.nbp-goods-color dd a.current em{border: none; width: 24px; height: 24px;}
.nbp-goods-info dl.nbp-goods-count dd strong{display: inline-block; border: 1px #ccc solid; border-radius: 25px; overflow: hidden;}
.nbp-goods-info dl.nbp-goods-count dd strong b{width: 35px; height: 35px; overflow: hidden; display: inline-block; background-size: 20px; background-position: center center; background-repeat: no-repeat; cursor: pointer; vertical-align: middle;}
.nbp-goods-info dl.nbp-goods-count dd strong b.r{background-image: url(../images/nbp-goods-countr.jpg);}
.nbp-goods-info dl.nbp-goods-count dd strong b.p{background-image: url(../images/nbp-goods-countp.jpg);}
.nbp-goods-info dl.nbp-goods-count dd strong input{border: none; background: none; width: 2em; text-align: center; color: #333; font-size: 16px; padding: 2px 5px; vertical-align: middle;}
.nbp-goods-info .nbp-goods-price{margin-top: 30px;}
.nbp-goods-info .nbp-goods-price strong{display: inline-block; vertical-align: middle; font-size: 36px; font-weight: bold; line-height: 1em; margin-right: 10px;}
.nbp-goods-info .nbp-goods-price strong sup{display: inline-block; vertical-align: top; font-weight: normal; font-size: 18px; line-height: 1.6em;}
.nbp-goods-info .nbp-goods-price span{display: inline-block; vertical-align: bottom; line-height: 1.3em; color: #999; font-size: 18px; text-decoration: line-through;}
.nbp-goods-info .nbp-goods-btn{margin-top: 30px;}
.nbp-goods-info .nbp-goods-btn a{border: 1px #e7380d solid; color: #e7380d; border-radius: 4px; height: 35px; line-height: 35px; width: 10em; text-align: center; display: inline-block; font-size: 12px; -webkit-transition: all .3s ease; -moz-transition: all .3s ease; transition: all .3s ease; font-weight: bold;}
.nbp-goods-info .nbp-goods-btn a:hover{}
@media print, only screen and (-webkit-min-device-pixel-ratio: 1.5), screen and (min-resolution: 144dpi), screen and (min-resolution: 144dppx) {
    .nbp-goods-info dl.nbp-goods-count dd strong b.r{background-image: url(../images/nbp-goods-countr-x2.jpg);}
    .nbp-goods-info dl.nbp-goods-count dd strong b.p{background-image: url(../images/nbp-goods-countp-x2.jpg);}
}











