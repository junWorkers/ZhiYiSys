@charset "utf-8";
/* CSS Document */

*{
	padding:0;
	margin:0 auto;
}
a{
	color:#999;
	text-decoration:none;
	cursor:pointer;
	outline: 0 none;
	}
#fa-header{
	width:100%;
	height:61px;
	background:#fff;
	position:fixed;  /*固定位置*/
	border-bottom:#CCC 1px solid;
	z-index:1000;
}
#fa-center{
	float:left;
	padding-left:170px;
}
.fa-logo{
	float:left;	
}
.fa-logo img{
	padding-top:12px;
	width:120px;
}


ul,li{
		margin:0;
		padding:0;
}
.fa-centers {
		list-style:none;
		height:60px;
		line-height:30px;
		float:left;	
		padding-top:20px;
}
.fa-centers li{
	float:left;
}
.fa-centers li a{
		display:inline-block;
		width:80px;
		height:30px;
		text-decoration:none;
		font-size:16px;
		color:#999;
}
#fa-center ul li a:hover {
    color: #F00;
    text-decoration-color: -moz-use-text-color;
    text-decoration-line: none;
    text-decoration-style: solid;
}

.fa-centers .first1{
	padding-left:45px;
}
.fa-centers .last1{
	padding-left:30px;
}
#fa-center .img1{
	padding-top:10px;
	padding-left:20px;
	float:left;
}
.fa-right{
	list-style:none;
	height:60px;
	line-height:30px;
	float:left;	
	padding-top:20px;
}
.fa-right li{
	float:left;
}
.fa-right li a{
	display:inline-block;
	height:30px;
	text-decoration:none;
	font-size:14px;
	color:#999;
	padding-left:18px;
}
/*首部结束*/
.bg-store {
	padding-top:61px;
    background-attachment: scroll;
    background-clip: border-box;
    background-color: rgba(0, 0, 0, 0);
    background-image: url(../images/bg_store_new.jpg);
    background-origin: padding-box;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: auto auto;
    height: 299px;
    width: 100%;
}

/*地图*/

/*-----------*/
.container {
    margin-bottom: 0;
    margin-left: auto;
    margin-right: auto;
    margin-top: 540px;
    overflow-x: hidden;
    overflow-y: hidden;
    width: 1170px;
}
.store {
    overflow-x: hidden;
    overflow-y: hidden;
}
.store-news {
    overflow-x: hidden;
    overflow-y: hidden;
}
.store-news .st-list {
	list-style:none;
}
.store-news .st-list li {
    float: left;
    margin-right: 50px;
    width: 240px;
}
.store-news .st-list .dec {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: #fafafa;
    background-image: none;
    background-origin: padding-box;
    background-position: 0 0;
    background-repeat: repeat;
    background-size: auto auto;
    padding-bottom: 18px;
    padding-left: 18px;
    padding-right: 18px;
    padding-top: 18px;
}
.store-news .st-list .dec h1 {
    color: #2f383d;
    font-size: 18px;
    margin-bottom: 15px;
    text-align: center;
}
.store-news .st-list .dec p {
    color: #919293;
    font-size: 12px;
    line-height: 1.8;
}
<!--尾部-->
#nb-footer{
	width:100%;
	height:50px;
	float:left;
	border-top:#ccc 1px solid;
	background:#CCC;
}

#nb-footer{
	padding: 30px 0 50px 0; 
	margin: auto; z-index: 9; /*font-family: 'Shscn';*/
}
.nubia-copyright{
	margin: auto; 
	width: 1000px; 
	color: #ccc; 
	position: relative; 
	font-size: 12px;
}
.nubia-copyright .nubia-foot-nav {
	padding-top:8px; 
	-webkit-transform: scale(0.83333333) translateX(-99px);
	-moz-transform: scale(0.83333333) translateX(-99px);
	-ms-transform: scale(0.83333333) translateX(-99px);
	transform: scale(0.83333333) translateX(-99px);
}
.nubia-copyright .nubia-foot-nav a{
	padding: 0 15px; color: #999; 
	-webkit-transition: color .3s ease-in-out; 
	-moz-transition: color .3s ease-in-out; 
	-ms-transition: color .3s ease-in-out; 
	transition: color .3s ease-in-out;
}
.nubia-copyright .nubia-foot-nav a:first-child{
	padding-left: 0;
}
.nubia-copyright .nubia-foot-nav a:hover{
	color: #333;
}
.nubia-copyright .nb-footer-info{
	padding-top: 10px; 
	-webkit-transform: scale(0.83333333) translateX(-99px);
	-moz-transform: scale(0.83333333) translateX(-99px);
	-ms-transform: scale(0.83333333) translateX(-99px);
	transform: scale(0.83333333) translateX(-99px);
}
.nubia-copyright .nb-footer-info a{
	display: inline-block; 
	margin-right: 20px; 
	color: #ccc; 
	-webkit-transition: color .3s ease-in-out; 
	-moz-transition: color .3s ease-in-out; 
	-ms-transition: color .3s ease-in-out; 
	transition: color .3s ease-in-out;
}
.nubia-copyright .nb-footer-info a:hover{
	color: #333;
}
.nubia-copyright span.nb-footer-func{
	position: absolute; 
	top: 0; 
	right: 0;
}
.nubia-copyright span.nb-footer-func a{
	width: 30px; 
	height: 30px; 
	overflow: hidden; 
	display: inline-block; 
	margin-left: 10px; 
	line-height: 1em; 
	position: relative;
}
.nubia-copyright span.nb-footer-func a.nb-en-ico{
	background-position: -96px 0;
}
.nubia-copyright span.nb-footer-func a.nb-weibo-ico{
	background-position: -126px 0;
}
.nubia-copyright span.nb-footer-func a.nb-weixin-ico{
	background-position: -156px 0;
}
.nubia-copyright span.nb-footer-func a.nb-apk-ico{
	background-position: -246px 0;
}
.nubia-copyright span.nb-footer-func a.nb-cloud-ico{
	background-position: -186px 0;
}
.nubia-copyright span.nb-footer-func a.nb-service-ico{
	background-position: -216px 0;
}
.nubia-copyright span.nb-footer-func a.nb-weibo-ico:hover{
	background-position: -126px -30px;
}
.nubia-copyright span.nb-footer-func a.nb-weixin-ico:hover{
	background-position: -156px -30px;
}
.nubia-copyright span.nb-footer-func a.nb-apk-ico:hover{
	background-position: -246px -30px;
}
.nubia-copyright span.nb-footer-func a.nb-cloud-ico:hover{
	background-position: -186px -30px;
}
.nubia-copyright span.nb-footer-func a.nb-service-ico:hover{
	background-position: -216px -30px;
}
.nubia-copyright span.nb-footer-func strong{
	width: 220px; 
	height: 30px; 
	display: inline-block; 
	vertical-align: middle; 
	overflow: hidden; 
	margin-left: 10px;
}
span.nb-footer-func .nb-footer-wx-layer{
	width: 150px; 
	height: 150px; 
	background: #fff; 
	border:1px #ccc solid; 
	position: absolute; 
	right: 219px; 
	bottom: 45px; 
	border-radius: 5px; 
	display: none;
}
span.nb-footer-func .nb-footer-wx-layer img{
	width: 130px; 
	height: 130px; 
	position: absolute; 
	top: 11px; 
	left: 10px; 
	z-index: 1;
}
span.nb-footer-func .nb-footer-wx-layer:before{
	width: 19px; 
	height: 10px; 
	overflow: hidden; 
	position: absolute; 
	bottom: -10px; 
	left: 50%; 
	margin-left: -9px; 
	content: ''; 
	z-index: 2; 
	background-size: 19px 10px; 
	background-repeat: no-repeat;
}
.experstore-layer{ position:absolute; magin-top:400px; margin-left:80px; z-index: 9; width: 1100px; height: 500px; background: #fff; border: 1px #e3e3e3 solid;}
.experstore-sw-tab{
	float:left;
	border-right: 1px solid #e3e3e3;
    display: block;
    height: 500px;
	width:800px;
	}
.experstore-sw-tab ul {
    
	list-style:none;
	border-bottom:1px solid #e3e3e3;

}

.experstore-sw-tab ul li a {
    border-right: 1px solid #fff;
    color: #333;
    display: block;
    font-size: 18px;
    margin: 0 20px;
    overflow: hidden;
    padding: 20px 0;
	
}
a {
    color: #999;
    cursor: pointer;
    outline: 0 none;
    text-decoration: none;
}
.experstore-sw-tab ul li.current{border-right: 1px #fff solid;}
.experstore-sw-tab ul li.current a{ color:#999; font-size:20px; font-weight:600;}
 .experstore-store-bigbox{width: 440px; height: 660px; float: right; padding: 20px 0;  background: #fff; background: rgba(255,255,255,.9);}
 .experstore-store .experstore-store-box{ height: 660px; overflow-y: auto;}
 .experstore-store-box ul{ padding: 0 40px;}
 .experstore-store-box li{padding: 30px 0 15px;}
 .experstore-store-box dt{font-size: 24px;}
 .experstore-store-box dd{margin-top: 10px; position: relative; margin-left:60px; float:left;}
 .experstore-store-box dd p{ color:#666;}
  .experstore-store-box dd img{ margin-left:-18px;}
 .experstore-store-box dd b{display: block; width: 16px; height: 14px; overflow: hidden; position: absolute; top: 23px; left: 0;}
 .experstore-store-box dd.add b{background-position: -32px 0;}
 .experstore-store-box dd.tel b{background-position: -49px 0;}
 .experstore-store-box dd.servise b{background-position: -67px 0;}
 .experstore-store-box dd.time b{background-position: -86px 0;}
 .experstore-store-box dd.traf b{background-position: -105px 0;}
 .experstore-store-box dd.list img{ margin-top:-10px; margin-left:-10px;}
.experstore-store-box dd.pri{  color:#666666; margin-top:80px; margin-left:-50px;}
.experstore-layer .experstore-store-box dl dd a.read-map-btn {
    background-color: #fff;
    border: 1px solid #cccccc;
    border-radius: 3px;
    color: #666666;
    display: inline-block;
    font-size: 14px;
    margin-top: 8px;
    overflow: hidden;
    padding: 8px 25px;
    transition: all 0.3s ease-in-out 0s;
}
