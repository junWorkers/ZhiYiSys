}
.app-list img{
	margin-bottom:5px;
}
.app-list h2{
	margin-bottom:5px;
	text-align:center;
	font-weight:bold;
	color:#000;
	width:175px;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	padding:0 3px;
	font:bold 16px/1.5'\5FAE\8F6F\96C5\9ED1','\5b8b\4f53';
}
.app-list .star{
	background:url(../images/star.jpg) no-repeat;
	width:100px;
	height:15px;
	margin:0 auto;
