.app-list .btn{
	background:url(../images/btn.gif) no-repeat;
	text-align:center;
	width:80px;
	height:30px;
	padding-bottom:30px;
	margin:0 auto;
	line-height:30px \9;
}
.container-app .app-list .btn a{
	font:12px/30px'\5FAE\8F6F\96C5\9ED1','\5b8b\4f53';
	color:#fff;
}
.rank .rank-app{
	overflow:hidden;
	zoom:1;
	width:210px;
}
.rank-app ul{
	margin-left:5px;
}
.rank-app li{
	margin:0 auto;
	margin-bottom:40px;
	overflow:hidden;
	zoom:1;
}
.rank-app li a{
	cursor:pointer;
	color:#000;
	font-weight:bold;
}
.rank-app h1{
	margin-left:10px;
	padding-bottom:14px;
	width:150px;
	font:bold 16px/1.5'\5FAE\8F6F\96C5\9ED1','\5b8b\4f53';
	color:#e8380d;
	margin-bottom:16px;
}
.rank-app .app-num{
	margin-top:25px;
	margin-right:10px;
	display:inline;
	float:left;
	font-weight:bold;
	color:#999999;
}
.rank-app img{
	margin-right:5px;
	float:left;
}
.rank-app ul li h2{
	float:left;
	margin-top:38px;
	width:100px;
	height:30px;
	overflow:hidden;
	font:14px/30px'\5FAE\8F6F\96C5\9ED1','\5b8b\4f53';
}
.rank-app ul li .star{
	background:url(../images/star.jpg) no-repeat;
	width:100px;
	height:15px;
	float:left;
	display:none;
}
.app-title ul{
	color:#e8380d;
	font:14px/30px'\5FAE\8F6F\96C5\9ED1','\5b8b\4f53';
	line-height:35px;
	margin-left:23px;
}
.app-title li{
	float:left;
}
.app-title li a{
	line-height:35px;
	display:block;
	float:left;
	color:#e8370d;
	font-weight:bold;
}
