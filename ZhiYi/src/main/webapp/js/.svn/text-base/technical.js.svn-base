function dijianlalala(num){
	document.getElementById("x"+num).style.paddingTop='100px';
	var a=document.getElementById("sidebar").getElementsByTagName("li")
	
	for(var i=1;i<=a.length;i++){
		if(i==num){
			var b=a[i-1];
			b.getElementsByTagName('a')[0].className='current';
		}else{
			var b=a[i-1];
			b.getElementsByTagName('a')[0].className='navfix';
		}
	}
}